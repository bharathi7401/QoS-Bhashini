apiVersion: 1

datasources:
  - name: InfluxDB-QoS-Metrics
    type: influxdb
    access: proxy
    url: http://influxdb:8086
    secureJsonData:
      token: ${INFLUXDB_GRAFANA_TOKEN}
    jsonData:
      version: Flux
      organization: ${INFLUXDB_ORG}
      defaultBucket: ${INFLUXDB_BUCKET}
      tlsSkipVerify: true
      httpMethod: POST
      queryTimeout: 60s
      maxSeries: 10000
