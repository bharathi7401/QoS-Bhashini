      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "green", "value": 80}
              ]
            },
            "unit": "percent",
            "decimals": 1
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 1}
