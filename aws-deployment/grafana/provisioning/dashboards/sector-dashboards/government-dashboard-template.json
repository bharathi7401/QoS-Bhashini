{
  "dashboard": {
  "id": null,
    "title": "Government Sector Dashboard - $tenant_name",
    "tags": ["government", "citizen-services", "compliance", "operational-efficiency"],
    "style": "dark",
    "timezone": "browser",
  "panels": [
    {
        "id": 1,
        "title": "Citizen Service Efficiency Overview",
        "type": "row",
        "collapsed": false,
        "panels": [],
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Average Service Completion Time",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(service_completion_time{tenant_id=\"$tenant_id\"})",
            "legendFormat": "{{service_type}}"
          }
        ],
      "fieldConfig": {
        "defaults": {
          "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 30},
                {"color": "red", "value": 60}
              ]
            },
            "unit": "m",
            "decimals": 1
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 1}
      },
      {
        "id": 3,
        "title": "Citizen Satisfaction Score",
        "type": "gauge",
        "targets": [
          {
            "expr": "avg(citizen_satisfaction{tenant_id=\"$tenant_id\"})",
            "legendFormat": "Overall Satisfaction"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 1,
            "max": 5,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 1},
                {"color": "yellow", "value": 3},
                {"color": "green", "value": 4}
              ]
            },
            "unit": "short",
            "decimals": 1
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 1}
      },
